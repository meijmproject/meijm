<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.yh.hr.component.gb.bo">
    <class name="com.yh.hr.component.gb.bo.GBPlan" table="YHG_GB_PLAN_DETAIL">
        <id name="jhgGbPlanDetailOid" column="YHG_GB_PLAN_DETAIL_OID" type="java.lang.Long">
            <generator class="identity"/>
        </id>

        <property name="unitOid" type="java.lang.Long">
            <column name="UNIT_OID">
                <comment>单位ID</comment>
            </column>
        </property>
        <property name="postType" type="java.lang.String">
            <column name="POST_TYPE">
                <comment>岗位类别</comment>
            </column>
        </property>
        <property name="postLevel" type="java.lang.String">
            <column name="POST_LEVEL">
                <comment>岗位级别</comment>
            </column>
        </property>
        <property name="postLevelCount" type="java.lang.Integer">
            <column name="POST_LEVEL_COUNT">
                <comment>岗位级别数量</comment>
            </column>
        </property>
        <property name="createdByCode" type="java.lang.String">
            <column name="CREATED_BY_CODE">
                <comment>CreatedByCode</comment>
            </column>
        </property>
        <property name="createdByName" type="java.lang.String">
            <column name="CREATED_BY_NAME">
                <comment>CreatedByName</comment>
            </column>
        </property>
        <property name="createdDate" type="java.util.Date">
            <column name="CREATED_DATE">
                <comment>CreatedDate</comment>
            </column>
        </property>
        <property name="updatedByCode" type="java.lang.String">
            <column name="UPDATED_BY_CODE">
                <comment>UpdatedByCode</comment>
            </column>
        </property>
        <property name="updatedByName" type="java.lang.String">
            <column name="UPDATED_BY_NAME">
                <comment>UpdatedByName</comment>
            </column>
        </property>
        <property name="updatedDate" type="java.util.Date">
            <column name="UPDATED_DATE">
                <comment>UpdatedDate</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
