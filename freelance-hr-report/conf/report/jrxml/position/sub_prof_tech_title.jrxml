	<queryString><![CDATA[select 
 certificate_no ,
 (select di.DIC_ITEM_NAME from yha_dic_item di,yha_dic_type dt where di.DIC_TYPE_OID= dt.DIC_TYPE_OID 
and dt.DIC_TYPE_CODE='YHRS0117' and di.DIC_ITEM_CODE = b.prof_Tech_Code ) as prof_Tech_Code
,(select di.DIC_ITEM_NAME from yha_dic_item di,yha_dic_type dt where di.DIC_TYPE_OID= dt.DIC_TYPE_OID 
and dt.DIC_TYPE_CODE='YHRS0047' and di.DIC_ITEM_CODE = b.prof_Tech_Level ) as prof_Tech_Level
,date_format(b.procure_Date,'%Y-%m') as procure_Date,
date_format(b.VALIDITY_DATE,'%Y-%m') as VALIDITY_DATE
,b.SPECIALITY_NAME as SPECIALITY_NAME
,issue_Organ,remark
 from JHC_PB_PROF_TECH_QUALIF_INFO b where 1=1
 and b.person_oid =$P{personOid} 
 order by b.procure_Date DESC]]></queryString>

