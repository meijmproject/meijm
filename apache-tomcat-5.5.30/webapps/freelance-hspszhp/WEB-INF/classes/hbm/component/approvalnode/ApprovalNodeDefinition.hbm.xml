<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.yh.component.approvalnode.bo">
    <class name="com.yh.component.approvalnode.bo.ApprovalNodeDefinition" table="yha_approval_node_definition">
        <id name="approvalNodeDefinitionOid" column="APPROVAL_NODE_DEFINITION_OID" type="java.lang.Long">
            <generator class="identity"/>
        </id>

        <property name="itemCode" type="java.lang.String">
            <column name="ITEM_CODE">
                <comment>事项编码</comment>
            </column>
        </property>
        <property name="itemName" type="java.lang.String">
            <column name="ITEM_NAME">
                <comment>事项名称</comment>
            </column>
        </property>
        <property name="itemNodeCode" type="java.lang.String">
            <column name="ITEM_NODE_CODE">
                <comment>事项环节编码</comment>
            </column>
        </property>
        <property name="itemNodeName" type="java.lang.String">
            <column name="ITEM_NODE_NAME">
                <comment>事项环节名称</comment>
            </column>
        </property>
        <property name="isActive" type="java.lang.String">
            <column name="IS_ACTIVE">
                <comment>是否激活</comment>
            </column>
        </property>
        <property name="remark" type="java.lang.String">
            <column name="REMARK">
                <comment>备注</comment>
            </column>
        </property>
        <property name="createdByCode" type="java.lang.String">
            <column name="CREATED_BY_CODE">
                <comment>创建人CODE</comment>
            </column>
        </property>
        <property name="createdByName" type="java.lang.String">
            <column name="CREATED_BY_NAME">
                <comment>创建人姓名</comment>
            </column>
        </property>
        <property name="createdDate" type="java.util.Date">
            <column name="CREATED_DATE">
                <comment>创建时间</comment>
            </column>
        </property>
        <property name="lastModifierCode" type="java.lang.String">
            <column name="LAST_MODIFIER_CODE">
                <comment>修改人CODE</comment>
            </column>
        </property>
        <property name="lastModifierName" type="java.lang.String">
            <column name="LAST_MODIFIER_NAME">
                <comment>修改人姓名</comment>
            </column>
        </property>
        <property name="lastModifyDate" type="java.util.Date">
            <column name="LAST_MODIFY_DATE">
                <comment>修改日期</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
