<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.yh.admin.bo">
    <class name="com.yh.admin.bo.RoleFunc" table="YHB_ROLE_FUNCTION">
      <composite-id>   
        <key-property name="roleId" column="ROLE_ID" type="long" />
        <key-property name="funcId" column="FUNC_ID" type="long"/>
      </composite-id>
    </class>
</hibernate-mapping>