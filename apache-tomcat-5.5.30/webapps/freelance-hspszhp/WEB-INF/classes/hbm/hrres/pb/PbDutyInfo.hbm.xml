<?xml version="1.0" encoding='UTF-8'?>


<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.yh.hr.res.pb.bo">
    <class name="com.yh.hr.res.pb.bo.PbDutyInfo" table="YHC_PB_DUTY_INFO">
        <id name="dutyOid" column="DUTY_OID" type="java.lang.Long">
            <generator class="identity"/>
        </id>

        <property name="personOid" type="java.lang.Long">
            <column name="PERSON_OID">
                <comment>人员ID</comment>
            </column>
        </property>
        <property name="dutyStatus" type="java.lang.String">
            <column name="DUTY_STATUS">
                <comment>在聘状态</comment>
            </column>
        </property>
        <property name="dutyName" type="java.lang.String">
            <column name="DUTY_NAME">
                <comment>职务名称</comment>
            </column>
        </property>
        <property name="deptOid" type="java.lang.Long">
            <column name="DEPT_OID">
                <comment>任职部门</comment>
            </column>
        </property>
        <property name="startDate" type="java.util.Date">
            <column name="START_DATE">
                <comment>任职时间</comment>
            </column>
        </property>
        <property name="endDate" type="java.util.Date">
            <column name="END_DATE">
                <comment>离任时间</comment>
            </column>
        </property>
        <property name="remark" type="java.lang.String">
            <column name="REMARK">
                <comment>备注</comment>
            </column>
        </property>
        <property name="createBy" type="java.lang.String">
            <column name="CREATE_BY">
                <comment>创建人ID</comment>
            </column>
        </property>
        <property name="createName" type="java.lang.String">
            <column name="CREATE_NAME">
                <comment>创建人名称</comment>
            </column>
        </property>
        <property name="createDate" type="java.util.Date">
            <column name="CREATE_DATE">
                <comment>创建时间</comment>
            </column>
        </property>
        <property name="updateBy" type="java.lang.String">
            <column name="UPDATE_BY">
                <comment>修改人ID</comment>
            </column>
        </property>
        <property name="updateName" type="java.lang.String">
            <column name="UPDATE_NAME">
                <comment>修改人名称</comment>
            </column>
        </property>
        <property name="updateDate" type="java.util.Date">
            <column name="UPDATE_DATE">
                <comment>修改时间</comment>
            </column>
        </property>
        <property name="isMainDutyInfo" type="java.lang.String">
            <column name="IS_MAIN_DUTY_INFO">
                <comment>是否主要职务</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
