<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.yh.hr.res.pt.bo">
    <class name="com.yh.hr.res.pt.bo.PtWorkInjuryVacation" table="JHC_PT_VACATION_WORKINJURY">
        <id name="vacationOid" column="VACATION_OID" type="java.lang.Long">
            <generator class="assigned"/>
        </id>
        <property name="isAffirmWorkInjury" type="java.lang.String">
            <column name="IS_AFFIRM_WORKINJURY">
                <comment>是否认定工伤</comment>
            </column>
        </property>
        <property name="workInjuryAffirmUnit" type="java.lang.String">
            <column name="WORKINJURY_AFFIRM_UNIT">
                <comment>工伤认定单位</comment>
            </column>
        </property>
        <property name="createBy" type="java.lang.String">
            <column name="CREATE_BY">
                <comment>创建人ID</comment>
            </column>
        </property>
        <property name="createName" type="java.lang.String">
            <column name="CREATE_NAME">
                <comment>创建人名称</comment>
            </column>
        </property>
        <property name="createDate" type="java.util.Date">
            <column name="CREATE_DATE">
                <comment>创建时间</comment>
            </column>
        </property>
        <property name="updateBy" type="java.lang.String">
            <column name="UPDATE_BY">
                <comment>修改人ID</comment>
            </column>
        </property>
        <property name="updateName" type="java.lang.String">
            <column name="UPDATE_NAME">
                <comment>修改人名称</comment>
            </column>
        </property>
        <property name="updateDate" type="java.util.Date">
            <column name="UPDATE_DATE">
                <comment>修改时间</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
